menuconfig MODULES
	bool "Enable loadable module support"
	option modules
	default y

config A
	bool "A"
	depends on C
	#select S
	#default y

config B
	bool "B"
	depends on A && D

config C
	bool "C"
	depends on D
	default y if E
	default m

config D
	bool "D"

config E
	bool "E"
	select D

config S
	tristate "S"
	depends on D && I=16

config I
	int "I"
